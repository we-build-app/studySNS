<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.softcon.studysns.view.home.login.LoginIDFindResult" >

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="57dp"
        android:layout_height="67dp"
        android:src="?android:attr/textCheckMark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.388" />

    <TextView
        android:id="@+id/FindIDResult_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="홍길동"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.292"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.052" />

    <TextView
        android:id="@+id/FindIDResult_Description1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="님의 아이디는"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toEndOf="@+id/FindIDResult_username"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.052" />

    <TextView
        android:id="@+id/FindIDResult_userID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="youitdfg****"
        android:textColor="@color/main_skyblue"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.356"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FindIDResult_username"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/FindIDResult_Description2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="입니다"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toEndOf="@+id/FindIDResult_userID"
        app:layout_constraintTop_toBottomOf="@+id/FindIDResult_Description1"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/FindIDResult_TologinButton"
        android:layout_width="360dp"
        android:layout_height="60dp"
        android:background="@drawable/register_complete_round"
        android:backgroundTint="@color/main_skyblue"
        android:text="로그인 하기"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FindIDResult_userID"
        app:layout_constraintVertical_bias="0.89" />
</androidx.constraintlayout.widget.ConstraintLayout>